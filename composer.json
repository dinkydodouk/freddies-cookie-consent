        "handle": "freddies-cookie-consent",
        "developer": "Freddie Dodo",
        "developerUrl": "https://www.dinkydodo.com",
        "documentationUrl": "https://github.com/dinkydodouk/freddies-cookie-consent/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/dinkydodouk/freddies-cookie-consent/master/CHANGELOG.md",
        "class": "dinkydodouk\\freddiescookieconsent\\FreddiesCookieConsent"
