{
    "name": "dinkydodouk/freddies-cookie-consent",
    "description": "A cookie consent banner that blocks cookies before they are set.",
    "type": "craft-plugin",
    "version": "1.0.2",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "freddies cookie consent"
    ],
    "support": {
        "docs": "https://github.com/dinkydodouk/freddies-cookie-consent/blob/master/README.md",
        "issues": "https://github.com/dinkydodouk/freddies-cookie-consent/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Freddie Dodo",
            "homepage": "https://www.dinkydodo.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
